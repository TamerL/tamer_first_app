<p><%= @user.name %></p>
<p><%= @user.email %></p>
<p><%= @user.age %></p>
<%= button_to 'Delete', @user, method: :delete, data: {confirm: 'Are you sure?'} %>

<h3>All Posts</h3>
<% @posts.each do |post| %>
  <p><%= post.name %>
  <p><%= post.title %>
<% end %>

<h3>All Cars</h3>
<% @cars.each do |car| %>
  <p><%= car.model %>
  <p><%= car.colour %>
<% end %>
